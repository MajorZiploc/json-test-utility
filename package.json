{
  "name": "json-test-utility",
  "version": "0.0.9",
  "description": "JSON utility for refactoring, comparing, and migrating json test data.",
  "main": "./build/index.js",
  "types": "./index.d.ts",
  "repository": "https://github.com/MajorZiploc/json-test-utility",
  "author": "Manyu Lakhotia",
  "license": "MIT",
  "scripts": {
    "specs-jr": "node -r ts-node/register node_modules/tape/bin/tape src/spec/JsonRefactor.test.ts | tap-spec",
    "specs-jc": "node -r ts-node/register node_modules/tape/bin/tape src/spec/JsonComparer.test.ts | tap-spec",
    "specs-jm": "node -r ts-node/register node_modules/tape/bin/tape src/spec/JsonMigration.test.ts | tap-spec",
    "specs": "node -r ts-node/register node_modules/tape/bin/tape src/spec/*.test.ts | tap-spec"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.151",
    "@types/request-promise-native": "^1.0.17",
    "@types/tape": "^4.13.0",
    "@types/fs-extra": "^9.0.0",
    "@types/tape-catch": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.0",
    "tape-catch": "^1.0.6",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.15"
  }
}
